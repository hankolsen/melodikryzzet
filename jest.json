{
  "moduleDirectories": ["node_modules", "src"],
  "moduleNameMapper": {
    "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/configs/jest/fileMock.js",
    "\\.(css|scss|less)$": "<rootDir>/configs/jest/styleMock.js"
  },
  "transform": {
    "\\.(ts|tsx)$": "ts-jest"
  }
}
